{
  "name": "@zagrajmy/app",
  "license": "MIT",
  "version": "0.1.0-alpha.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lib": "tsc -p ./tsconfig.lib.json && babel dist-ts -d dist && rm -rf dist-ts",
    "start": "next start",
    "lint": "eslint {src,pages}/**/*.{ts,tsx}",
    "lint:fix": "yarn lint --fix",
    "typecheck": "yarn cross-env NODE_OPTIONS=--max_old_space_size=4096 tsc --noEmit --skipLibCheck",
    "ci": "yarn typecheck && yarn lint",
    "ts-node": "ts-node -P scripts/tsconfig.json",
    "get-schema": "yarn ts-node scripts/getSchema",
    "codegen:zeus": "zeus data/schema.graphql --ts ./data",
    "codegen": "yarn get-schema && yarn codegen:zeus && yarn format",
    "deploy:app": "now",
    "hasura:console": "cd data/throwaway/hasura && hasura console",
    "format": "prettier {pages,src,data,scripts}/**/*.{ts,tsx,graphql,gql} --write",
    "madge:circular": "npx madge --circular --ts-config ./tsconfig.json .",
    "test:e2e": "yarn cypress open",
    "test:e2e:record": "concurrently --kill-others --names \"app,cy\" \"yarn build && yarn start\" \"yarn wait-on http://localhost:3000/ && yarn dotenv-load cypress run --record\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.11.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@next/mdx": "^9.3.2",
    "@styled-system/css": "^5.1.5",
    "@theme-ui/color": "^0.4.0-alpha.0",
    "@theme-ui/core": "^0.4.0-alpha.0",
    "@theme-ui/css": "^0.4.0-alpha.0",
    "@types/auth0": "^2.20.2",
    "@types/cookie": "^0.3.3",
    "@types/node": "^13.9.5",
    "@types/node-fetch": "^2.5.5",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-dom": "^16.9.5",
    "@types/styled-system": "^5.1.9",
    "@types/styled-system__css": "^5.0.6",
    "@unly/universal-language-detector": "^2.0.3",
    "@zeit/next-css": "^1.0.1",
    "auth0": "^2.24.0",
    "babel-plugin-emotion": "^10.0.29",
    "babel-plugin-styled-components": "^1.10.7",
    "date-fns": "2.11.1",
    "docz": "^2.2.0",
    "dotenv": "^8.2.0",
    "fp-ts": "^2.5.3",
    "i18next": "^19.3.4",
    "io-ts": "^2.1.2",
    "io-ts-reporters": "^1.0.0",
    "next": "9.3.3",
    "next-fonts": "^1.0.3",
    "next-next-link": "^0.0.1",
    "node-fetch": "^2.6.0",
    "nom-ts": "^0.1.2",
    "nookies": "^2.2.1",
    "patch-package": "^6.2.2",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-datepicker": "^2.14.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.3",
    "react-hook-form": "^5.2.0",
    "react-i18next": "^11.3.4",
    "react-window-infinite-loader": "^1.0.5",
    "styled-system": "^5.1.5",
    "swr": "^0.2.0",
    "theme-ui": "^0.4.0-alpha.0",
    "ts-essentials": "^6.0.3",
    "ts-node": "^8.8.0",
    "use-auth0-hooks": "^0.7.0",
    "use-onclickoutside": "^0.3.1",
    "utility-types": "^3.10.0",
    "snyk": "^1.316.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.2.1",
    "cypress": "^4.5.0",
    "egzek": "^1.2.0",
    "graphql": "^14.6.0",
    "graphql-compiler": "^1.7.0",
    "graphql-zeus": "^2.6.3",
    "graphqurl": "^0.3.3",
    "now": "^17.1.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3"
  },
  "snyk": true
}
