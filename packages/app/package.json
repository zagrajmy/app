{
  "name": "@zagrajmy/app",
  "license": "MIT",
  "version": "0.1.0-alpha.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:lib": "tsc -p ./tsconfig.lib.json && babel dist-ts -d dist && rm -rf dist-ts",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:lib:tsc": "tsc -p ./tsconfig.lib.json -w",
    "dev:lib:babel": "cross-env CHOKIDAR_USEPOLLING=true babel --watch --verbose --extensions '.ts,.js,.jsx' dist-ts -d dist",
    "start": "next start",
    "lint": "eslint {src,pages,test}/**/*.{ts,tsx}",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:tests": "yarn tsc --noEmit --skipLibCheck -p ./test",
    "ci": "yarn typecheck && yarn lint",
    "ts-node": "ts-node -P scripts/tsconfig.json",
    "get-schema": "yarn ts-node scripts/getSchema",
    "codegen:zeus": "zeus data/schema.graphql --ts ./data",
    "codegen": "yarn get-schema && yarn codegen:zeus && yarn format",
    "deploy:app": "vercel",
    "hasura:console": "cd data/throwaway/hasura && hasura console",
    "format": "prettier {pages,src,data,scripts}/**/*.{ts,tsx,graphql,gql} --write",
    "madge:circular": "npx madge --circular --ts-config ./tsconfig.json .",
    "test:e2e": "yarn cypress open",
    "test:e2e:run": "concurrently --success first --kill-others --names \"app,cy\" \"yarn start\" \"yarn wait-on http://localhost:3000/ && yarn dotenv-load cypress run\"",
    "snyk-protect": "snyk protect",
    "prepublish": "timeout 60s yarn snyk-protect || echo '‚è∞ snyk protect timed out.'",
    "analyze-bundle": "cross-env ANALYZE=true yarn build"
  },
  "main": "dist/src/index.js",
  "module": "dist/src/index.esm.js",
  "types": "dist/src/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist"
  ],
