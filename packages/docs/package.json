{
  "name": "@zagrajmy/docs",
  "private": true,
  "description": "docs for the reusable code and styleguide",
  "version": "0.1.0-alpha.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "cd ../app && npm run build:lib",
    "build": "gatsby build",
    "dev": "gatsby develop -p 3001",
    "deploy": "npm run build && now --prod",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "shadow": "gatsby-theme eject",
    "format": "prettier src/**/*.{ts,tsx,js,jsx,graphql,gql} --write",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint --fix"
  },
  "dependencies": {
    "@theme-ui/prism": "^0.3.0",
    "@theme-ui/style-guide": "^0.3.0",
    "@zagrajmy/app": "^0.1.0-alpha.0",
    "docz": "^2.2.0",
    "gatsby": "^2.20.8",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-theme": "^0.1.2",
    "gatsby-theme-docz": "^2.2.0",
    "next-router": "^1.3.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "resolutions": {
    "theme-ui": "^0.4.0-alpha.0"
  },
  "bugs": {
    "url": "https://github.com/zagrajmy/frontend/issues"
  }
}
