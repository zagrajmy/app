    "@zagrajmy/app": "^0.1.0-alpha.0",
    "babel-preset-gatsby": "^0.4.6",
    "docz": "^2.3.1",
    "gatsby": "^2.22.4",
    "gatsby-plugin-typescript": "^2.4.3",
    "gatsby-source-filesystem": "^2.3.7",
    "gatsby-theme": "^0.1.1",
    "gatsby-theme-docz": "2.2.0",
    "next-router": "^1.3.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "resolutions": {
    "theme-ui": "^0.4.0-alpha.0"
